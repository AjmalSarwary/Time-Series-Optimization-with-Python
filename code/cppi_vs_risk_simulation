        "\n",
        "# Plot 'CPPI' with solid lines using the color palette\n",
        "ax = btr['Wealth'].plot(title='Simulation of Investment Alternatives under CPPI vs. Full Risk'\n",
        "                                            ,figsize=(12,6), color=[palette(i) for i in range(3)])\n",
        "\n",
        "# Plot 'Full Risk' with dashed lines using the same color palette\n",
        "btr['Risky Wealth'].plot(ax=ax, style='--', color=[palette(i) for i in range(3)])\n",
        "\n",
        "# Set the legend to reflect both 'CPPI' and 'Full Risk'\n",
        "handles, labels = ax.get_legend_handles_labels()\n",
        "new_labels = []\n",
        "for i in range(len(labels)//2):\n",
        "    new_labels.append(f\"CPPI - {labels[i]}\")\n",
        "    new_labels.append(f\"Full Risk - {labels[i]}\")\n",
        "\n",
